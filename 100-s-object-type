public Boolean isTypeAccount(sObject record)
{
    // code here
    Boolean res;
    List<sObject> A = New List<sObject>();
    record = New Account();
    A.add(record);
    record = New Contact();
    A.add(record);
    if(A.get(0) instanceof Account)
    {
        res = true;
    }
    if(A.get(0) instanceof Contact)
    {
        res = false;
    }
    
    return res;
}