public Boolean isTypeAccount(sObject record)
{
    // code here
    Boolean res;
    List<Account> A = New List<Account>();
    Account record = New Account();
    A.add(record);
    if(A.get(0) instanceof Account)
    {
        res = true;
    }
    else
    {
        res = false;
    }
    
    return res;
}