public Boolean isPrime(Integer num) {
    //code here
    Boolean res;
    Integer k;
    For( Integer i = 2 ; i < num ; i++)
    {
        if(Math.mod(num, i ) == 0) 
        {
            k = 0;
        }
        else
        {
            k = 1;
        }
    } 
    if (k = 0)
    {
        res = false;
    } 
    else
    {
       res = true;
    }


    return res;
}