public String divide(Integer a, Integer b){
    //code here
    String res;
    Integer div = a/b;
   try
   {
       if(a > 0 && b>0)
       {
          res = String.valueof(div);
              
       }
       

    }
    catch(Exception e)
    {
      
        res = e.getMessage();
       
    }
  
  return res;
}