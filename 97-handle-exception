public String divide(Integer a, Integer b){
    //code here
    String res;
    Integer div = a/b;
    if(b==0)
    {
        res = 'Divide by '+String.valueof(b);
    }
    else if(a != Null && b!= Null && b!= 0)
    {
        res = String.valueof(div);
    }
    else if(a == Null)
    {
        res= 'Attempt to de-reference a null object';
    }

    return res;
}