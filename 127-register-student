public String registerStudent(String name, String phone, String email) {
    // code here
    String res;
    apxio__Student__c S = New apxio__Student__c();
    S.Name = name;
    S.apxio__Phone__c = phone;
    S.apxio__Email__c = email;
   
   Insert S;
   List<String> RNL;
   For(apxio__Student__c ES: [Select id, apxio__Registration_Number__c from apxio__Student__c])
   {
     RNL.Add(apxio__Registration_Number__c);
   }

   return RNL;
    
}