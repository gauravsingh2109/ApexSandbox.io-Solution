public List<apxio__Student__c> selectStudentsWithoutContactInfo() {
    // code here
    List<apxio__Student__c> SL = [Select id, name, apxio__Email__c, apxio__Phone__c,apxio__Active__c,apxio__Registration_Number__c from apxio__Student__c where apxio__Active__c = True AND apxio__Phone__c = Null OR apxio__Email__c = Null ORDER BY Name ASC];
    return SL;
}
