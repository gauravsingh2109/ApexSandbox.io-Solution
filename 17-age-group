public String ageGroup(Integer n) {
    //code here
    String res;
    if( n >=0 && n <=1)
    {
       res = 'Infant';   
    }
    else if( n>1 && n<15)
    {
        res = 'Child';
    }
    else if( n>14 && n<22)
    {
        res = 'Youth';
    }
    else
    {
         res = 'Adult';
    }

    return res;
}