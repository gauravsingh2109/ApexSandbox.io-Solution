public Integer fibonacci(Integer n) {
    //code here
    Integer sum = 0;
    Integer fact = 0;
    Integer i;
    Integer num3;
    Integer num1 = 0;
    Integer num2 = 1;
        for (i = 0; i < n; i++) 
        {
            num3 = num2 + num1;
            num1 = num2;
            num2 = num3;
        }
    
    return num3;
}