public List<apxio__Student__c> selectActiveStudents() {
    // code here
    List<apxio__Student__c> SL  = [Select apxio__Active__c from apxio__Student__c where apxio__Active__c = true];
    For(apxio__Student__c ES :SL )
    {
        If(SL.apxio__Active__c == true)
        {
            SL.Add(ES.apxio__Active__c)
        }
      return SL;
    }
}