public String convert15to18DigitId(String fifteenDigit)
{
    String res; 
    Id ids;
    
    //Integer i = fifteenDigit.Length();
 
   if(fifteenDigit.length()!=null)
   {
        if( fifteenDigit.Length() < 15)
    {
        res= '-1';
    }
    if(fifteenDigit.Length() >= 15)
    {
        ids = Id.valueOf(fifteenDigit);
        res = String.valueOf(ids);
    }
    
   }
   if(fifteenDigit.length()==null)
   {
         ids = null;
        res = null;
    
   }
   

    return res;
}