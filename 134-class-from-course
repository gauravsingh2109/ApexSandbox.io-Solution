public Id classFromCourse(String courseName) {
    // code here
    apxio__Class__c C = New apxio__Class__c();
     C.Name = courseName;
     C.apxio__Course__c = courseName;

    Insert C;
   
    List<apxio__Course__c>  CL = [Select Id, Name,apxio__Course__c, apxio__Course__c.apxio__Class__c from apxio__Course__c];

    Return CL.Id;

}