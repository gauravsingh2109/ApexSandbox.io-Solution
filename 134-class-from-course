public Id classFromCourse(String courseName) {
    // code here
    List<apxio__Course__c> CL = [Select Id, Name From apxio__Course__c where Name=:courseName Limit 1];

    if(CL.size()== 0)
    {
        return null;
    }
    else
    {
       apxio__Class__c C = New apxio__Class__c();
       C.Name = CL[0].Name;
       C.apxio__Course__c = CL[0.Id];
       Insert C;

       Return C.Id;
    }

}