public Id classFromCourse(String courseName) {
    // code here
    apxio__Class__c C = New apxio__Class__c();
     C.Name = courseName;
     C.apxio__Course__c = courseName;

    Insert C;
   
    List<apxio__Class__c>  CL = [Select Id, Name, apxio__Class__c.apxio__Course__c from apxio__Class__c];

    Return CL.Id;

}