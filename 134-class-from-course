public Id classFromCourse(String courseName) {
    // code here
    apxio__Class__c C = New apxio__Class__c();
     C.Name = courseName;
     C.apxio__Course__c = courseName;

    Insert C;
   
   List<apxio__Course__c> CL = [SELECT Id, Name(SELECT Id, Name FROM apxio__Class__c WHERE Id = :C.Id) FROM apxio__Course__c WHERE Id = :apxio__Course__c.Id];
}