public Id insertCourse(String name, String details, Integer credits) {
    // code here
    apxio__Course__c C = New apxio__Course__c();
    C.Name  = name;
    C.apxio__Course_Details__c  = details;
    if(credits<5)
    {
      C.apxio__Credits__c = ''+credits;
      Insert C;
    }
    else
    {
        C.apxio__Credits__c = null;
    
    }
    
    

    return C;
}