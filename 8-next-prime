public Integer nextPrime(Integer num) {
    //code here
    Integer k;
    Integer flag;
    Integer res;
    For( k = num; k < (num+10); k++)
    {
        For(Integer i = 2; i<k; i++)
        {
            if(Math.mod(k,i) != 0)
            {
               flag = 1;
            }
            
        }
    
    }
    
    if(flag == 1)
    {
        res = k;
    }
   return res; 
}
    
