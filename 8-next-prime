public Integer nextPrime(Integer num) {
    //code here
    num++;
    while(true)
    {
        if(IsPrime(num))
        {
            return num;
        }
        num++;
    }

}

public Boolean IsPrime(Integer n)
{
    if(n<=1)
    {
        return false;
    }
    for(Integer i = 2; i<n; i++)
    {
        if(Math.mod(n, i) == 0)
        {
            return false;
        }
    }
    return true;

}
    
