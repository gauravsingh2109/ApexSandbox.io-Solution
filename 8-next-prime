public Integer nextPrime(Integer num) {
    //code here
    Integer k;
    Integer flag;
    Integer res;
    For( k = num; k < (num+100); k++)
    {
         For( Integer i = 2 ; i < num ; i++)
        {  
           if(Math.mod(k, i ) != 0) 
            {
              res = k;
            }
        } 
        break;

    }
    
   return res; 
}
    
