public Integer nextPrime(Integer num) {
    //code here
    Integer k;
    Integer y;
    for (k = num; k < (num+10); k++ )
    {
       For( Integer i = 2 ; i < num ; i++)
      {
        if(Math.mod(num, i ) == 0) 
        {
            y = 0;
        }
      } 
      if (y != 0)
      {
        res = k ;
      } 
    }

    return k;
    
}
    
