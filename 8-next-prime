public Integer nextPrime(Integer num) {
    //code here
    Integer k;
    Integer l;
    integer x;
    Integer res;
    Boolean b;
    For( k = num; k>= num; k++)
    {
        for( l = 2; l< k; l++)
        {
            if(Math.mod(k,l) != 0)
            {
                b = true;
                if(b==true)
                {
                    res= k;
                }
                
            }
            
        }
    
    }
    
   return res; 
}
    
