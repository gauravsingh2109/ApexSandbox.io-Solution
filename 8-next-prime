public Integer nextPrime(Integer num) {
    //code here
    Integer k;
    Integer l;
    Integer res;
    For( k = num; k > num; k++)
    {
        for( l = 2; l< k; l++)
        {
            if(Math.mod(k,l) != 0)
            {
                res = k;
            }
            break;
        }
    
    }
    
   return res; 
}
    
